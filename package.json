{
    "name": "@urlbox/n8n-nodes-urlbox",
    "version": "0.1.2",
    "description": "Urlbox's n8n community node for generating full page screenshots, converting to PDFs, and video recordings.",
    "license": "MIT",
    "homepage": "https://urlbox.com",
    "keywords": [
        "n8n-community-node-package",
        "screenshot",
        "scrape",
        "pdf",
        "video",
        "html"
    ],
    "author": {
        "name": "Urlbox Ltd.",
        "email": "support@urlbox.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/urlbox/n8n-nodes-urlbox.git"
    },
    "main": "index.js",
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/UrlboxApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Urlbox/Urlbox.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "publishConfig": {
        "access": "public"
    }
}
